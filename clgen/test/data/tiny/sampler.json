{
  "kernels": {
    "language": "opencl",
    "args": [
      "__global float*",
      "__global float*",
      "__global float*",
      "const int"
    ],
    "max_length": 512,  # max sample length
    "temperature": 1,  # sampling temperature [0,1]
    "seed": null  # if an integer, seed the RNG
  },
  "sampler": {
    "min_samples": 2000,  # stop sampling after min_kernels samples (ignored if <= 0)
    "min_kernels": -1,  # stop sampling after min_kernels good samples (ignored if <= 0)
    "static_checker": true,  # use static checker {true,false}
    "gpuverify": false  # use GPUVerify {true,false}
  }
}
